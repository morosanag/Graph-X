<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/calcpan"
    android:orientation="vertical"
    tools:context=".GraphActivity" >

    <LinearLayout
        android:id="@+id/layoutu"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1.9"
        android:baselineAligned="false" >

        <EditText
            android:id="@+id/edittext"
            style="@android:style/Widget.Holo.Button.Borderless"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:alpha="0.7"
            android:background="@drawable/layout_d"
            android:clickable="false"
            android:cursorVisible="false"
            android:focusable="false"
            android:textColor="#E18739"
            android:textSize="37dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="2"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="2" >

            <Button
                android:id="@+id/lg"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="lg"
                android:textColor="#E18739"
                android:textSize="20dp" />

            <Button
                android:id="@+id/ln"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="ln"
                android:textColor="#E18739"
                android:textSize="20dp" />

            <Button
                android:id="@+id/lb"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="lb"
                android:textColor="#E18739"
                android:textSize="20dp" />

            <Button
                android:id="@+id/buttonsqrt"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="sqrt"
                android:textColor="#E18739"
                android:textSize="20dp" />

            <Button
                android:id="@+id/floor"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="floor"
                android:textColor="#E18739"
                android:textSize="20dp" />

            <Button
                android:id="@+id/frac"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="frac"
                android:textColor="#E18739"
                android:textSize="20dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="2" >

            <Button
                android:id="@+id/sin"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="sin"
                android:textColor="#E18739"
                android:textSize="20dp" />

            <Button
                android:id="@+id/cos"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="cos"
                android:textColor="#E18739"
                android:textSize="20dp" />

            <Button
                android:id="@+id/tan"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="tan"
                android:textColor="#E18739"
                android:textSize="20dp" />

            <Button
                android:id="@+id/ctan"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="ctan"
                android:textColor="#E18739"
                android:textSize="20dp" />

            <Button
                android:id="@+id/abs"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="abs"
                android:textColor="#E18739"
                android:textSize="20dp" />

            <Button
                android:id="@+id/pi"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="Ï€"
                android:textColor="#E18739"
                android:textSize="20dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1.5" >

            <Button
                android:id="@+id/buttonegal"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_b"
                android:text="="
                android:textColor="#E18739"
                android:textSize="37dp" />

            <Button
                android:id="@+id/buttonUnknown"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_b"
                android:text="x"
                android:textColor="#E18739"
                android:textSize="37dp"
                android:textStyle="italic" />

            <Button
                android:id="@+id/buttonC"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_b"
                android:text="C"
                android:textColor="#E18739"
                android:textSize="37dp"
                android:textStyle="italic" />

            <Button
                android:id="@+id/buttonAdd"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="0.75"
                android:alpha="0.7"
                android:background="@drawable/layout_b"
                android:text="Add"
                android:textColor="#E18739"
                android:textSize="37dp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="2"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="2" >

            <Button
                android:id="@+id/button7"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="7"
                android:textColor="#E18739"
                android:textSize="37dp" />

            <Button
                android:id="@+id/button8"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="8"
                android:textColor="#E18739"
                android:textSize="37dp" />

            <Button
                android:id="@+id/button9"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="9"
                android:textColor="#E18739"
                android:textSize="37dp" />

            <Button
                android:id="@+id/buttonplus"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="+"
                android:textColor="#E18739"
                android:textSize="37dp" />

            <Button
                android:id="@+id/e"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="e"
                android:textColor="#E18739"
                android:textSize="37dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="2" >

            <Button
                android:id="@+id/button4"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="4"
                android:textColor="#E18739"
                android:textSize="37dp" />

            <Button
                android:id="@+id/button5"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="5"
                android:textColor="#E18739"
                android:textSize="37dp" />

            <Button
                android:id="@+id/button6"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="6"
                android:textColor="#E18739"
                android:textSize="37dp" />

            <Button
                android:id="@+id/buttonminus"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="-"
                android:textColor="#E18739"
                android:textSize="37dp" />

            <Button
                android:id="@+id/buttonexp"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="^"
                android:textColor="#E18739"
                android:textSize="20dp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="2" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="2" >

                <Button
                    android:id="@+id/button1"
                    style="@android:style/Widget.Holo.Button.Borderless"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_margin="0dp"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="5sp"
                    android:layout_weight="1"
                    android:alpha="0.7"
                    android:background="@drawable/layout_a"
                    android:text="1"
                    android:textColor="#E18739"
                    android:textSize="37dp"
                    tools:ignore="NestedWeights" />

                <Button
                    android:id="@+id/button2"
                    style="@android:style/Widget.Holo.Button.Borderless"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_margin="0dp"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="5sp"
                    android:layout_weight="1"
                    android:alpha="0.7"
                    android:background="@drawable/layout_a"
                    android:text="2"
                    android:textColor="#E18739"
                    android:textSize="37dp" />

                <Button
                    android:id="@+id/button3"
                    style="@android:style/Widget.Holo.Button.Borderless"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_margin="0dp"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="5sp"
                    android:layout_weight="1"
                    android:alpha="0.7"
                    android:background="@drawable/layout_a"
                    android:text="3"
                    android:textColor="#E18739"
                    android:textSize="37dp" />

                <Button
                    android:id="@+id/buttonx"
                    style="@android:style/Widget.Holo.Button.Borderless"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_margin="0dp"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="5sp"
                    android:layout_weight="1"
                    android:alpha="0.7"
                    android:background="@drawable/layout_a"
                    android:text="*"
                    android:textColor="#E18739"
                    android:textSize="37dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="2" >

                <Button
                    android:id="@+id/button0"
                    style="@android:style/Widget.Holo.Button.Borderless"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_margin="0dp"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="5sp"
                    android:layout_weight="1.3"
                    android:alpha="0.7"
                    android:background="@drawable/layout_a"
                    android:text="0"
                    android:textColor="#E18739"
                    android:textSize="37dp" />

                <Button
                    android:id="@+id/buttonpoint"
                    style="@android:style/Widget.Holo.Button.Borderless"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_margin="0dp"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="5sp"
                    android:layout_weight="2"
                    android:alpha="0.7"
                    android:background="@drawable/layout_a"
                    android:text="."
                    android:textColor="#E18739"
                    android:textSize="37dp" />

                <Button
                    android:id="@+id/buttondiv"
                    style="@android:style/Widget.Holo.Button.Borderless"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_margin="0dp"
                    android:layout_marginLeft="5sp"
                    android:layout_marginTop="5sp"
                    android:layout_weight="2"
                    android:alpha="0.7"
                    android:background="@drawable/layout_a"
                    android:text="/"
                    android:textColor="#E18739"
                    android:textSize="37dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="4"
            android:orientation="vertical" >

            <Button
                android:id="@+id/open"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text="("
                android:textColor="#E18739"
                android:textSize="20dp" />

            <Button
                android:id="@+id/close"
                style="@android:style/Widget.Holo.Button.Borderless"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_margin="0dp"
                android:layout_marginLeft="5sp"
                android:layout_marginTop="5sp"
                android:layout_weight="1"
                android:alpha="0.7"
                android:background="@drawable/layout_a"
                android:text=")"
                android:textColor="#E18739"
                android:textSize="20dp" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>